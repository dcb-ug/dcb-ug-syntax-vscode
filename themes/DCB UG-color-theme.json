{
  "name": "DCB UG",
  "type": "dark",
  "colors": {
    "editor.background": "#18171b",
    "editor.foreground": "#a8afb3",
    "activityBarBadge.background": "#567a8a",
    "sideBarTitle.foreground": "#a8a6a2"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "foreground": "#505152", "fontStyle": "italic" }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "string constant.other.placeholder",
        "meta.var-single-variable.expr.js"
      ],
      "settings": { "foreground": "#a8afb3" }
    },
    {
      "name": "Object Literal Keys",
      "scope": [
        "punctuation.definition.block.js.meta.objectliteral.js",
        "meta.object-literal.key.js"
      ],
      "settings": { "foreground": "#567a8a" }
    },
    {
      "name": "Color Literals",
      "scope": ["constant.other.color"],
      "settings": { "foreground": "#8fc161" }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": { "foreground": "#ff5370" }
    },
    {
      "name": "Keywords & Storage",
      "scope": [
        "keyword",
        "storage.modifier",
        "storage.type.class.python",
        "storage.type.function.python"
      ],
      "settings": { "foreground": "#e66084" }
    },
    {
      "name": "Control Flow",
      "scope": ["keyword.control", "keyword.control.flow"],
      "settings": { "foreground": "#ff5555" }
    },
    {
      "name": "Operators & Punctuation",
      "scope": [
        "punctuation",
        "punctuation.definition.tag",
        "punctuation.separator.inheritance.php",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.section.embedded",
        "keyword.other.template",
        "keyword.other.substitution"
      ],
      "settings": { "foreground": "#567a8a" }
    },
    {
      "name": "HTML/XML Tags",
      "scope": [
        "entity.name.tag",
        "meta.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter"
      ],
      "settings": { "foreground": "#ff5555" }
    },
    {
      "name": "Functions & Methods",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method",
        "entity.name.method.js",
        "meta.class-method.js entity.name.function.js",
        "variable.function.constructor"
      ],
      "settings": { "foreground": "#427988", "fontStyle": "italic" }
    },
    {
      "name": "Block-Scope Variables",
      "scope": ["meta.block variable.other"],
      "settings": { "foreground": "#a8afb3" }
    },
    {
      "name": "Numbers & Constants",
      "scope": [
        "constant.numeric",
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape",
        "keyword.other.unit",
        "keyword.other"
      ],
      "settings": { "foreground": "#8ebb60" }
    },
    {
      "name": "Strings & Symbols",
      "scope": [
        "string",
        "string.quoted.single",
        "string.quoted.double",
        "string.quoted.single.python",
        "string.quoted.double.python",
        "constant.other.symbol",
        "constant.other.key",
        "markup.heading",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": { "foreground": "#ad9811" }
    },
    {
      "name": "Classes & Types",
      "scope": [
        "entity.name.class",
        "entity.name.type.class",
        "entity.other.inherited-class"
      ],
      "settings": { "foreground": "#356369", "fontStyle": "bold" }
    },
    {
      "name": "CSS Property Names",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name"
      ],
      "settings": { "foreground": "#567a8a" }
    },
    {
      "name": "Module & Member Imports",
      "scope": [
        "entity.name.module.js",
        "variable.import.parameter.js",
        "variable.other.class.js",
        "keyword.control.import.python"
      ],
      "settings": { "foreground": "#e66084" }
    },
    {
      "name": "Language-Level Methods",
      "scope": ["variable.language"],
      "settings": { "foreground": "#e66084", "fontStyle": "italic" }
    },
    {
      "name": "Attributes",
      "scope": [
        "entity.other.attribute-name",
        "text.html.basic entity.other.attribute-name.html",
        "text.html.basic entity.other.attribute-name"
      ],
      "settings": { "foreground": "#567a8a", "fontStyle": "italic" }
    },
    {
      "name": "Inserted Lines",
      "scope": ["markup.inserted"],
      "settings": { "foreground": "#89b859" }
    },
    {
      "name": "Deleted Lines",
      "scope": ["markup.deleted"],
      "settings": { "foreground": "#ef7177" }
    },
    {
      "name": "Changed Lines",
      "scope": ["markup.changed"],
      "settings": { "foreground": "#e66084" }
    },
    {
      "name": "Regular Expressions",
      "scope": ["string.regexp"],
      "settings": { "foreground": "#8ebb60" }
    },
    {
      "name": "Escape Sequences",
      "scope": ["constant.character.escape"],
      "settings": { "foreground": "#567a8a" }
    },
    {
      "name": "URLs & Links",
      "scope": ["*url*", "*link*", "*uri*"],
      "settings": { "fontStyle": "underline" }
    },
    {
      "name": "Decorators",
      "scope": [
        "tag.decorator.js entity.name.tag.js",
        "tag.decorator.js punctuation.definition.tag.js",
        "meta.decorator.python",
        "meta.decorator.identifier.python"
      ],
      "settings": {
        "foreground": "#539d8c",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Python Docstrings",
      "scope": [
        "string.quoted.docstring.single.python",
        "string.quoted.docstring.multi.python",
        "string.quoted.double.docstring.python"
      ],
      "settings": { "foreground": "#505152", "fontStyle": "italic" }
    },
    {
      "name": "Python Type Hints",
      "scope": [
        "storage.type.annotation.python",
        "entity.name.type.python",
        "entity.name.type",
        "support.type.python",
        "meta.function.return-type.python entity.name.type",
        "meta.function.return-type.python entity.name.class",
        "meta.annotation.python entity.name.type",
        "meta.annotation.python entity.name.class"
      ],
      "settings": {
        "foreground": "#539d8c"
      }
    }
  ]
}
